  "homepage": "https://github.com/callisto1337/grid#readme",
  "peerDependencies": {
    "vue": ">=3.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.8",
    "@vitejs/plugin-vue": "^4.3.4",
    "prettier": "^3.0.3",
    "typescript": "5.0.4",
    "vite": "^4.5.0",
    "vite-plugin-dts": "^3.6.1",
    "vue": "^3.3.7",
    "vue-tsc": "^1.8.20"
